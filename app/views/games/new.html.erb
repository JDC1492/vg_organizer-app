<h1>New game? Add it to the collection!!<h1>
<%= form_with model: @game, local: true  do |f| %>
<%= f.label :title, "Game Title" %>
<%= f.text_field :title, @game.title %>
<br>
<%= f.label :release_year, "Year of Release" %>
<%= f.text_field :release_year, @game.release_year %>
<br>
<%= f.label :description, "What's it About?" %>
<%= f.text_field :description, @game.description %>
<br>
<%= f.label :genre, "Genre" %>
<%= f.text_field :genre, @game. %>
<br>
<%= f.label :developer, "Developer" %>
<%= f.text_field :developer, @game. %>
<br>
<%= f.label :complete, "Finished?" %>
<%= f.check_box :complete, @game. %>
<br>

<!--  -->

<%= f.submit %>
<% end %>